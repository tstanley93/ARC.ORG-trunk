body {
background: #0099CC;
}

td
{
	color: #FFFFFF;
	font-weight: normal;
	font-family: verdana;
}

h1, h2, h3, h4, h5 {

    color: #FFFFFF;
    margin: 0px;
}

.DynamicMenuItemStyle
{
    border-width: 1px;
    border-color: #FFFFFF;
    font-size: 14px;
    font-weight: bold;
    border-bottom-style: solid;
}

