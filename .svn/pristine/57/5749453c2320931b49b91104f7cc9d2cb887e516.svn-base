    margin: 4px;
}

.BlogDiv
{
	width: 300px;
	border: 2px solid white;
	text-align: justify;
	padding: 4px;
}

.BlogTitle
{
	font-size: 18px;
}

.DynamicMenuItem
{
    border-width: 1px;
    border-color: #FFFFFF;
    z-index: 200;
    border-top-style: solid;
    border-right-style: solid;
    border-left-style: solid;
}

h1 
{
    font-size: 35px;
}

h2 
{
    text-align: center;
}

h3
{
    font-size: 14px;
}

.calendar_title
{
	background-color: #6A6A6A;
	font-family: Verdana;
	font-size: larger;
	font-style: normal;
	color: #000000;
	font-weight: bold;
}

.span_font_h3
{
	font-size: 14px;
	color: #ffb400;
}

.staff
{
	font-size: 14px;
	color: #FFFFFF;
	margin: 0px;
}

.staff_link
{
	font-size: 14px;
	margin: 0px;
	text-decoration: underline;
	color: #ffb400;
}



